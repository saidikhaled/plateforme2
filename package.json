{
	"name": "express-b",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"server": "babel-node ./src/bin/www",
		"server:prod": "node ./dist/bin/www",
		"dev": "env NODE_ENV=development npm-run-all server",
		"clean": "rimraf dist",
		"build": "babel ./src --out-dir dist",
		"prod": "env NODE_ENV=production npm-run-all clean build server:prod",
		"watch": "nodemon"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.8.2",
		"alert-node": "^1.2.4",
		"bcryptjs": "^2.4.3",
		"bootstrap": "^4.3.1",
		"connect-flash": "^0.1.1",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"ejs": "~2.5.7",
		"express": "~4.16.0",
		"express-ejs-layouts": "^2.5.0",
		"express-mysql-session": "^2.1.0",
		"express-session": "^1.16.1",
		"express-validator": "^5.3.1",
		"http-errors": "~1.6.2",
		"jquery": "^3.4.1",
		"morgan": "~1.9.0",
		"multer": "^1.4.1",
		"mysql": "^2.17.1",
		"npm-run-all": "^4.1.5",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"popper.js": "^1.15.0",
		"rimraf": "^2.6.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.4.4",
		"dotenv": "^8.0.0",
		"nodemon": "^1.19.0"
	}
}
